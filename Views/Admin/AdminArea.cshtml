<html class="not-ie" lang="en">
 <!--<![endif]-->
<head>
    <!-- Basic Meta Tags -->
    <meta charset="utf-8">
    <title>Muhammad Din Nursery Farm</title>
   
    <!--[if (gte IE 9)|!(IE)]>
      <meta http-equiv="X-UA-Compatible" content="IE=edge">
      <meta http-equiv="Content-Type" content="text/html;charset=utf-8">
    <![endif]-->
    <!-- Favicon -->
    <link href="~/img/favicon.ico" rel="icon" type="image/png">

    <!-- Styles -->
    <link href="../css/styles.css" rel="stylesheet">
    <link href="~/css/bootstrap-override.css" rel="stylesheet">

    <!-- Font Avesome Styles -->
    <link href="~/css/font-awesome/font-awesome.css" rel="stylesheet">
    <!--[if IE 7]>
        <link href="~/css/font-awesome/font-awesome-ie7.min.css" rel="stylesheet">
    <![endif]-->
    <!-- FlexSlider Style -->
    <link rel="stylesheet" href="~/css/flexslider.css" type="text/css" media="screen">

    <!-- Internet Explorer condition - HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

</head>
<body>
    <!-- Header -->
    <header id="header">
        <div class="container">
            <div class="row t-container">

                <!-- Logo -->
                <div class="span3">
                    <div class="logo">
                        <a href="index"><img src="~/img/logo-header.png" alt=""></a>
                    </div>
                </div>

                <div class="span9">
                    <div class="row space60"></div>
                    <nav id="nav" role="navigation">
                        <a href="#nav" title="Show navigation">Show navigation</a>
                        <a href="#" title="Hide navigation">Hide navigation</a>
                        <ul class="clearfix">
                            <li class="active"><a href="~/Admin/messages" title="" style="font-weight:700">Messages</a></li>
                            <li><a href="~/Admin/chPassword" title="" style="font-weight:700">Ch Password</a></li>
                            <li><a href="~/Admin/addUser" title="" style="font-weight:700">Add user</a></li>
                            <li>
                                <a href="" title="" style="font-weight:700">Add Photos<span></span></a>
                                <ul>
                                    <!-- Submenu -->

                                    <li><a href="~/Admin/addphotos/Indoor" title="" style="font-weight:700">Indoor</a></li>
                                    <li><a href="~/Admin/addphotos/Outdoor" title="" style="font-weight:700">Outdoor</a></li>
                                    <li><a href="~/Admin/addphotos/Trees" title="" style="font-weight:700">Trees</a></li>
                                    <li><a href="~/Admin/addphotos/Bonsai" title="" style="font-weight:700">Bonsai</a></li>
                                    <li><a href="~/Admin/addphotos/Criper" title="" style="font-weight:700">Cripers</a></li>
                                    <li><a href="~/Admin/addphotos/Shrubs" title="" style="font-weight:700">Shrubs</a></li>
                                </ul> <!-- End Submenu -->
                            </li>
                            
                            <li>
                                <a href="" title="" style="font-weight:700"><span>See photos</span></a>
                                <ul>
                                    <!-- Submenu -->

                                    <li><a href="~/Admin/seePhotos/Indoor" title="" style="font-weight:700">Indoor</a></li>
                                    <li><a href="~/Admin/seePhotos/Outdoor" title=" " style=" font-weight:700">Outdoor</a></li>
                                    <li><a href="~/Admin/seePhotos/Trees"  title=" " style=" font-weight:700">Trees</a></li>
                                    <li><a href="~/Admin/seePhotos/Bonsai"  title=" " style=" font-weight:700">Bonsai</a></li>
                                    <li><a href="~/Admin/seePhotos/Criper" title=" " style=" font-weight:700">Cripers</a></li>
                                    <li><a href="~/Admin/seePhotos/shrubs" title="" style="font-weight:700">Shrubs</a></li>
                                </ul> <!-- End Submenu -->
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
            <div class="row" style="margin-left:auto;margin-top:auto;float:right;">
                <form method="post" action="~/Admin/logout">
                    <input type="submit" class="btn btn-success btn-xs" value="Logout">


                </form>
                
            </div>&nbsp;&nbsp;&nbsp;
            <div class="row" style="margin-left:auto;margin-top:auto;float:right;">

                <form method="post" action="~/Admin/Order">
                    <input type="submit" class="btn btn-success btn-xs" value="Chek Order">
                </form>

</div>

                <div class="row space40"></div>
                <div class="slider1 flexslider">
                    <!-- Slider -->
                    @Html.Partial("_mainslider")
                    <!-- Slider End -->
                </div>
            </div>
    </header>
    <!-- Header End -->
    <!-- Content -->
    <div id="content">
        <div class="container">
            <div class="f-center">

                <h1 style="color: #fff; background-color: #008000; font-weight: 700">Welcome to @Session["user"]</h1>

                <div class="head-info" style="color: #800000; font-weight: 700; font-size: large; margin-top: 2%; ">
                    <em>MUHAMMAD DIN NURSERY FARM Settled in 1980. It is located in Pattoki. As Pattoki is a huge market of Plants of Every Kind. We are providing a large variety of Plants of each kind. So contact us to Register your order and buy plants online. We are online 24 hours a day.</em>
                </div>
            </div>
           

            <div class="space40"></div>

            <!-- Our Clients -->
            <!-- Our Clients End -->

            <div class="space50"></div>

        </div>
    </div>
    <!-- Content End -->
    <!-- Footer -->

              
    <!-- Footer End -->
    <!-- JavaScripts -->
    <script type="text/javascript" src="~/js/jquery-1.8.3.min.js"></script>
    <script type="text/javascript" src="~/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/js/functions.js"></script>
    <script type="text/javascript" defer src="~/js/jquery.flexslider.js"></script>

</body>
</html>
